{
  "name": "@k4i/eslint-config",
  "version": "2.0.0",
  "description": "My custom ESLint config",
  "author": "k4itrun",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./base": {
      "import": "./dist/src/configs/base.js",
      "types": "./dist/src/configs/base.d.ts"
    },
    "./astro": {
      "import": "./dist/src/configs/astro.js",
      "types": "./dist/src/configs/astro.d.ts"
    },
    "./prettier": {
      "import": "./dist/src/configs/prettier.js",
      "types": "./dist/src/configs/prettier.d.ts"
    },
    "./next": {
      "import": "./dist/src/configs/next.js",
      "types": "./dist/src/configs/next.d.ts"
    },
    "./node": {
      "import": "./dist/src/configs/node.js",
      "types": "./dist/src/configs/node.d.ts"
    },
    "./react": {
      "import": "./dist/src/configs/react.js",
      "types": "./dist/src/configs/react.d.ts"
    },
    "./typescript": {
      "import": "./dist/src/configs/typescript.js",
      "types": "./dist/src/configs/typescript.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "publish": "pnpm publish --access public --no-git-checks --provenance=false",
    "publish:next": "pnpm publish --access public --no-git-checks --provenance=false --tag next",
    "publish:test": "pnpm publish --access public --no-git-checks --provenance=false --dry-run",
    "publish:git": "pnpm publish --access public --no-git-checks --provenance=true",
    "publish:git:next": "pnpm publish --access public --no-git-checks --provenance=true --tag next",
    "publish:git:test": "pnpm publish --access public --no-git-checks --provenance=true --dry-run",
    "format": "prettier . --write --cache --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "1.23.2",
    "@eslint/compat": "1.2.5",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.19.0",
    "@next/eslint-plugin-next": "15.1.6",
    "@stylistic/eslint-plugin": "2.13.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/parser": "8.22.0",
    "eslint-config-flat-gitignore": "1.0.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-flat-config-utils": "1.1.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import-x": "4.6.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-tailwindcss": "3.18.0",
    "globals": "15.14.0",
    "typescript-eslint": "8.22.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/node": "22.12.0",
    "@types/rollup-plugin-auto-external": "2.0.5",
    "rimraf": "^6.0.1",
    "rollup": "4.32.1",
    "rollup-plugin-auto-external": "2.0.0"
  },
  "peerDependencies": {
    "eslint": "^9"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "keywords": [
    "javascript",
    "linting",
    "code-quality",
    "js-linting",
    "coding-standards",
    "typescript-linting",
    "eslint",
    "eslint-config",
    "styleguide",
    "config"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k4itrun/shared-configs.git"
  },
  "bugs": {
    "url": "https://github.com/k4itrun/shared-configs/issues"
  },
  "homepage": "https://github.com/k4itrun/shared-configs/tree/main/packages/eslint-config#readme"
}
